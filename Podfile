platform :ios, '10.3'
target 'Lambent' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Lambent
pod 'Firebase/Core'
pod 'Firebase/Database'
pod 'GoogleAnalytics'
pod 'SwiftLint'

target 'LambentUITests' do
    inherit! :search_paths
end

target 'LambentUnitTests' do
    inherit! :search_paths
end

end
